{
  "name": "create-next-app",
  "version": "1.0.0",
  "scripts": {
    "precommit": "lint-staged",
    "build": "next build",
    "start": "nodemon -w server.js",
    "production": "npm run build && NODE_ENV=production node index.js",
    "eslint": "eslint ./**/*.js",
    "format":
      "prettier --trailing-comma es5 --single-quote --write './*/*.js' './*/!(node_modules)/**/*.js'"
  },
  "lint-staged": {
    "*.js": ["prettier --trailing-comma es5 --single-quote --write", "git add"]
  },
  "dependencies": {
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "express": "^4.16.2",
    "isomorphic-fetch": "^2.2.1",
    "next": "^4.1.4",
    "next-redux-wrapper": "^1.3.4",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "styled-components": "^2.2.3"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.7.2",
    "eslint-config-react-app": "^2.0.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.2",
    "prettier": "^1.7.0"
  }
}
